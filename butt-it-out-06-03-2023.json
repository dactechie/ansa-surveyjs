            {
              "value": "4",
              "text": "I'm making progress"
            },
            {
              "value": "5",
              "text": "I'm managing really well - successfully quitting or reducing"
            },
            {
              "value": "6",
              "text": "I have managed to quit smoking"
            }
          ],
          "otherText": "I'm making progress"
