          "requiredIf": "{DoYouSmoke} = 'Cigarettes'",
          "choices": [
            {
              "value": "1",
              "text": "Not at all"
            },
            {
              "value": "2",
              "text": "A little "
            },
            {
              "value": "3",
              "text": "Willing to try"
            },
            {
              "value": "4",
              "text": "Quite motivated"
            },
            {
              "value": "5",
              "text": "Extremely motivated"
            }
