            {
              "value": "SOME",
              "text": "I knew that Directions could provide some assistance, but didn't have much detail"
            },
            {
              "value": "ABIT",
              "text": "I knew Directions could provide some quit smoking support"
            },
            {
              "value": "LOTS",
              "text": "I knew Directions could provide a range of specialist supports to help me quit smoking"
            }
          ]
        }
